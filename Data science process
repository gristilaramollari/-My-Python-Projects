   "source": [
    "married_status = df.loc[new_df['MARRIAGE'] == 1,:] \n",
    "single_status = df.loc[new_df['MARRIAGE'] == 2,:]\n",
    "divorced_status = df.loc[new_df['MARRIAGE'] == 3,:]\n",
    "others_status = df.loc[new_df['MARRIAGE'] == 0,:]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 220,
   "id": "4c9cd321",
   "metadata": {},
   "outputs": [],
   "source": [
    "# DEFAULT_DATA \n",
    "married_status_default = default_data.loc[default_data['MARRIAGE'] == 1,:] \n",
    "single_status_default = default_data.loc[default_data['MARRIAGE'] == 2,:] \n",
    "divorced_status_default = default_data.loc[default_data['MARRIAGE'] == 3,:] \n",
    "others_status_default = default_data.loc[default_data['MARRIAGE'] == 0,:]\n",
    "\n",
    "# not default_data\n",
    "married_status_not_default = not_default_data.loc[not_default_data['MARRIAGE'] == 1,:] \n",
    "single_status_not_default = not_default_data.loc[not_default_data['MARRIAGE'] == 2,:] \n",
    "divorced_status_not_default = not_default_data.loc[not_default_data['MARRIAGE'] == 3,:] \n",
    "others_status_not_default = not_default_data.loc[not_default_data['MARRIAGE'] == 0,:]\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 221,
   "id": "1a2b673c",
