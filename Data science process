    "\n",
    "# we loop through social status, check if the value is (0,1,2,3)\n",
    "# then assign to each of the columns the average payment\n",
    "\n",
    "for i in range(0, len(new_df)):\n",
    "    \n",
    "    if new_df['MARRIAGE'][i] == 0:\n",
    "        new_df['others'][i] = new_df['Pay Avg'][i]\n",
    "        \n",
    "    elif new_df['MARRIAGE'][i] == 1:\n",
    "        new_df['married'][i] = new_df['Pay Avg'][i]\n",
    "        \n",
    "    elif new_df['MARRIAGE'][i] == 2:\n",
    "        new_df['single'][i] = new_df['Pay Avg'][i] \n",
    "        \n",
    "    elif new_df['MARRIAGE'][i] == 3:\n",
    "        new_df['divorce'][i] = new_df['Pay Avg'][i]\n"
   ]
