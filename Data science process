    "# Replace infinite updated data with nan\n",
    "default_customers.replace([np.inf, -np.inf], np.nan, inplace=True)\n",
    "# Drop rows with NaN\n",
    "default_customers.dropna(inplace=True) #inplace=True is used to update the existing DataFrame. \n",
    "# Using replace method to select non-null rows\n",
    "default_customers = default_customers[default_customers.replace([np.inf, -np.inf], np.nan).notnull().all(axis=1)] \n",
    " \n",
    "print(default_customers)"
   ]
