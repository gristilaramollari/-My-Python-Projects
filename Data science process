    "ax[1,1].set(xlabel='Age Spread', ylabel='Count',\n",
    "       title='others', ylim=(0,20), xlim= (20,85))\n",
    "\n",
    "\n",
    "fig.tight_layout()\n",
    "st.set_y(1)\n",
    "fig.subplots_adjust(top=0.9)\n",
    "\n",
