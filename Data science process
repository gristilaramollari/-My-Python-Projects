    "fig, ax = plt.subplots(2,2, figsize=(8, 7)) \n",
    "st = fig.suptitle(\"Comparison between STATUS: DEFAULT & NOT DEFAULT\", fontsize=\"large\")\n",
    "\n",
    "plt.title('Comparison between STATUS: DEFAULT & NOT DEFAULT') \n",
    "# Chart for MARRIED\n",
    "ax[0,0].hist(married_status_default['AGE'], color = \"orange\", alpha= 0.9)\n",
    "ax[0,0].hist(married_status_not_default['AGE'], color= \"lightblue\", alpha = 0.5)\n",
    "ax[0,0].set(xlabel='Age Spread', ylabel='Count',\n",
    "       title='married', ylim=(0,500), xlim= (20,85))\n",
    " \n",
    "# Chart for SINGLE\n",
    "ax[0,1].hist(single_status_default ['AGE'], color = \"orange\", alpha= 0.9)\n",
    "ax[0,1].hist(single_status_not_default['AGE'], color= \"lightblue\", alpha = 0.5)\n",
    "ax[0,1].set(xlabel='Age Spread', ylabel='Count',\n",
