    "# Finding the average of bill, pay and stat "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 200,
   "id": "19c2a807",
   "metadata": {},
   "outputs": [],
   "source": [
    "new_df['Bill Avg'] = (new_df['Bill Apr'] + new_df['Bill May'] + new_df['Bill Jun']+ new_df['Bill Jul']+ new_df['Bill Aug']+ new_df['Bill Sep'])/6\n",
    "new_df['Pay Avg'] = (new_df['Pay Apr'] + new_df['Pay May'] + new_df['Pay Jun']+ new_df['Pay Jul']+ new_df['Pay Aug']+ new_df['Pay Sep'])/6 \n",
    "new_df['Stat Avg'] = (new_df['Stat Apr'] + new_df['Stat May'] + new_df['Stat Jun']+ new_df['Stat Jul']+ new_df['Stat Aug']+ new_df['Stat Sep'])/6"
   ]
  },
